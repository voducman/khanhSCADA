<!doctype html>
<html lang="en">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="/stylesheets/bootstrap.min.css">
    <!-- Font Awesome CSS -->
    <link rel="stylesheet" href="/stylesheets/all.css">
    <!-- Bootstrap form helpers -->
    <link rel="stylesheet" href="/stylesheets/bootstrap-formhelpers.min.css">
    <!-- Bootstrap slider -->
    <link rel="stylesheet" href="/stylesheets/bootstrap-slider.min.css">
    <!-- RGBA color picker -->
    <link type="text/css" rel="stylesheet" href="/stylesheets/rgbaColorPicker.css" />
    <!-- Data table -->
    <link type="text/css" rel="stylesheet" href="/stylesheets/dataTables.bootstrap4.min.css" />
    <!-- Own style -->
    <link rel="stylesheet" href="/stylesheets/Mystyle/style.css">

    <title>Main page</title>
</head>

<body onbeforeunload="return confirm('Are you sure to reload?')">
    <!------------------------ MAIN PAGE ------------------------->
    <nav class="navbar navbar-expand-sm bg-dark navbar-dark ">
        <a class="navbar-brand mx-2" href="/device" onclick="return confirm('Return to device page?')">
            <i class="fa fa-home"> HCMUT</i>
        </a>

        <div class="btn-toolbar mr-auto" role="toolbar" aria-label="Toolbar with button groups">
            <!-- <div class="btn-group" role="group" aria-label="Clipboard group">
                <button type="button" class="btn btn-dark" data-toggle="modal" data-target="#gaugeModal"><i
                        class="far fa-file mr-2"></i>Demo</button>
            </div> -->
            <div class="btn-group" role="group" aria-label="Clipboard group">
                <button type="button" class="btn btn-dark" id="btnOpen" data-toggle="modal" data-target="#openModal"><i
                        class="far fa-folder-open mr-2"></i>Open</button>
            </div>
            <div class="btn-group" role="group" aria-label="Clipboard group">
                <button type="button" class="btn btn-dark" data-toggle="modal" id="btnSave" data-target="#saveModal"><i
                        class="far fa-save mr-2"></i>Save</button>
            </div>
            <div class="btn-group" role="group" aria-label="Clipboard group">
                <button id="btnSetting" type="button" class="btn btn-dark" data-toggle="modal"
                    data-target="#settingModal"><i class="fas fa-cog mr-2"></i>Settings</button>
            </div>
            <!-- <div class="btn-group" role="group" aria-label="Clipboard group">
                <button type="button" class="btn btn-dark"><i class="fas fa-undo mr-2"></i>Undo</button>
            </div> -->
            <!-- <div class="btn-group" role="group" aria-label="Clipboard group">
                <button type="button" class="btn btn-dark"><i class="fas fa-redo mr-2"></i>Redo</button>
            </div> -->
            <div class="btn-group" role="group" aria-label="Clipboard group">
                <button type="button" class="btn btn-dark" id="btnRun"><i class="fas fa-play mr-2"></i>Run</button>
            </div>
            <div class="btn-group" role="group" aria-label="Clipboard group">
                <button type="button" class="btn btn-dark" disabled id="btnStop"><i
                        class="fas fa-stop mr-2"></i></i>Stop</button>
            </div>
            <div class="btn-group" role="group" aria-label="Clipboard group">
                <button type="button" class="btn btn-dark" id="btnPublish"><i
                        class="fas fa-upload mr-2"></i>Publish</button>
            </div>

            <!-- <div class="btn-group" role="group" aria-label="Clipboard group">
                <button type="button" class="btn btn-dark" id="btnTest"><i
                        class="fas fa-upload mr-2"></i>Test</button>
            </div> -->

            <!-- <div class="btn-group" role="group" aria-label="Clipboard group">
                <button type="button" class="btn btn-dark" id="btnTransform"><i
                        class="fas fa-upload mr-2"></i>Transform</button>
            </div> -->

        </div>

        <div class="btn-toolbar" role="toolbar" aria-label="Toolbar with button groups">
            <% if (deviceID) { %>
            <div hidden id="deviceID"><%= deviceID %></div>
            <% } %>
            <% if (user) { %>
            <div class="my-auto text-white" id="user"><%= user %></div>
            <% } %>
            <div class="btn-group" role="group" aria-label="Clipboard group">
                <a class="btn btn-dark" href="/logout" onclick="return confirm('Are you sure to logout?')"><i
                        class="fas fa-sign-out-alt mr-2"></i>Logout</a>
            </div>
        </div>
    </nav>

    <div class="container-fluid row mx-0 pl-0 pr-0">

        <!-- toolBoxPanel -->
        <div id="toolBoxPanle" class="col-md-2 mt-1 mx-0 px-0">
            <div class="card bg-dark no">
                <div class="card-header bg-secondary  text-white"><strong>Toolbox</strong></div>
                <div class="card-body no-gutter-all ">
                    <div id="accordion">
                        <!-- Basic objects -->
                        <div class="card">
                            <div class="card-header">
                                <a class="card-link" data-toggle="collapse" href="#collapseOne">
                                    <i class="fa" aria-hidden="true"></i>
                                    Basic objects
                                </a>
                            </div>
                            <div id="collapseOne" class="collapse show">
                                <div class="card-body">
                                    <div class="justify-content-sm-end justify-content-center">
                                        <button type="button" class="btn button-icon" data-toggle="tooltip" title="Line"
                                            onclick="startDraw('line')">
                                            <img src="/images/toolboxIcon/line.png" class="img-icon" alt="Line">
                                        </button>

                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Polyline" onclick="drawPolyline()">
                                            <img src="/images/toolboxIcon/polyline.png" class="img-icon" alt="Polyline">
                                        </button>

                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Ellipse" onclick="startDraw('ellipse')">
                                            <img src="/images/toolboxIcon/ellipse.png" class="img-icon" alt="Ellipse">
                                        </button>

                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Circle" onclick="startDraw('circle')">
                                            <img src="/images/toolboxIcon/circle.png" class="img-icon" alt="Circle">
                                        </button>

                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Rectangle" onclick="startDraw('rect')">
                                            <img src="/images/toolboxIcon/rectangle.png" class="img-icon"
                                                alt="Rectangle">
                                        </button>

                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Round rectangle" onclick="startDraw('roundRect')">
                                            <img src="/images/toolboxIcon/rounded-rectangle.png" class="img-icon"
                                                alt="Round rectangle">
                                        </button>

                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Polygon" onclick="drawPolygon()">
                                            <img src="/images/toolboxIcon/polygon.png" class="img-icon" alt="Polygon">
                                        </button>

                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Textblock" onclick="addNewText()">
                                            <img src="/images/toolboxIcon/textblock.png" class="img-icon"
                                                alt="Textblock">
                                        </button>

                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Graph view" onclick="addNewImage()">
                                            <img src="/images/toolboxIcon/graph-view.png" class="img-icon"
                                                alt="Graph view">
                                        </button>

                                    </div> <!-- /Wrapper div -->
                                </div> <!-- /Card-body div -->
                            </div> <!-- /collapseOne div -->
                        </div> <!-- /Card Basic objects div -->


                        <!-- Elements -->
                        <div class="card">
                            <div class="card-header">
                                <a class="collapsed card-link" data-toggle="collapse" href="#collapseTwo">
                                    <i class="fa" aria-hidden="true"></i>
                                    Elements
                                </a>
                            </div>
                            <div id="collapseTwo" class="collapse">
                                <div class="card-body">
                                    <div class="justify-content-sm-end justify-content-center">
                                        <!-- Display Value -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Display value" onclick="addNewDisplayValue()">
                                            <img src="/images/toolboxIcon/display-value.png" class="img-icon"
                                                alt="Display value">
                                        </button>
                                        <!-- /Display Value -->

                                        <!-- Button -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Button" onclick="addNewButton()">
                                            <img src="/images/toolboxIcon/button.png" class="img-icon" alt="Button">
                                        </button>
                                        <!-- /Button -->

                                        <!-- Switch -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Switch" onclick="addNewSwitch()">
                                            <img src="/images/toolboxIcon/toggle.png" class="img-icon" alt="Switch">
                                        </button>
                                        <!-- /Switch -->

                                        <!-- Input -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Input" onclick="addNewInput()">
                                            <img src="/images/toolboxIcon/input.png" class="img-icon" alt="Input">
                                        </button>
                                        <!-- /Input -->

                                        <!-- Slider -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Slider" onclick="addNewSlider()">
                                            <img src="/images/toolboxIcon/slider.png" class="img-icon" alt="Slider">
                                        </button>
                                        <!-- /Slider -->

                                        <!-- Vertical Slider -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Vertical slider" onclick="addNewVerticalSlider()">
                                            <img src="/images/toolboxIcon/vertical-slider.png" class="img-icon"
                                                alt="Vertical Slider">
                                        </button>
                                        <!-- /Vertical Slider -->

                                        <!-- Process bar -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Progress bar" onclick="addNewProcessbar()">
                                            <img src="/images/toolboxIcon/process-bar.png" class="img-icon"
                                                alt="Process bar">
                                        </button>
                                        <!-- /Process bar -->

                                        <!-- Vertical Process bar -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Vertical progress bar" onclick="addNewVerticalProcessbar()">
                                            <img src="/images/toolboxIcon/vertical-progress-bar.png" class="img-icon"
                                                alt="Vertical process bar">
                                        </button>
                                        <!-- /Vertical Process bar -->

                                        <!-- Checkbox -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Checkbox" onclick="addNewCheckbox()">
                                            <img src="/images/toolboxIcon/checkbox.png" class="img-icon" alt="Checkbox">
                                        </button>
                                        <!-- /Checkbox -->

                                        <!-- Symbol -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Symbol Set" onclick="addNewSymbolSet()">
                                            <img src="/images/toolboxIcon/symbol.png" class="img-icon" alt="Symbol">
                                        </button>
                                        <!-- /Symbol -->

                                        <!-- Chart -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Chart" onclick="addNewChart()">
                                            <img src="/images/toolboxIcon/trend.png" class="img-icon" alt="Trend">
                                        </button>
                                        <!-- /Chart -->

                                        <!-- Gauge -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Gauge" onclick="addNewGauge()">
                                            <img src="/images/toolboxIcon/gauge.png" class="img-icon" alt="Gauge">
                                        </button>
                                        <!-- /Gauge -->

                                    </div> <!-- /Wrapper div -->
                                </div> <!-- /Card-body div -->
                            </div> <!-- /collapseTwo div -->
                        </div> <!-- /Card Elements div -->

                        <!-- Dashboard only -->
                        <div class="card">
                            <div class="card-header">
                                <a class="collapsed card-link" data-toggle="collapse" href="#collapseThree">
                                    <i class="fa" aria-hidden="true"></i>
                                    Dashboard only
                                </a>
                            </div>
                            <div id="collapseThree" class="collapse">
                                <div class="card-body">
                                    <div class="justify-content-sm-end justify-content-center">
                                        <!-- Chart -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Chart dashboard" onclick="addNewChartDashboard()">
                                            <img src="/images/toolboxIcon/trend.png" class="img-icon" alt="Trend">
                                        </button>
                                        <!-- /Chart -->

                                        <!-- Gauge -->
                                        <button type="button" class="btn button-icon" data-toggle="tooltip"
                                            title="Gauge dashboard" onclick="addNewGaugeDashboard()">
                                            <img src="/images/toolboxIcon/gauge.png" class="img-icon" alt="Gauge">
                                        </button>
                                        <!-- /Gauge -->

                                    </div> <!-- /Wrapper div -->
                                </div> <!-- /Card-body div -->
                            </div> <!-- /collapseTwo div -->
                        </div> <!-- /Card Elements div -->
                    </div> <!-- /collapseGroup -->
                    <!-- /Collapse group -->
                </div>
            </div>
        </div>
        <!-- mainPagePanel -->
        <div id="mainPagePanel" class="col-md bg-dark my-1 no-gutter-margin">
            <div class="container-fluid no-gutter-all">
                <!-- Nav tabs -->
                <ul class="nav nav-tabs bg-secondary toggle-nav" role="tablist">
                    <li class="nav-item">
                        <a class="nav-link active" data-toggle="tab" href="#mainPage1"><i
                                class="fas fa-home mr-1"></i><strong>Home</strong></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link " data-toggle="tab" href="#alarm" id="alarmTitle"><i
                                class="fas fa-exclamation-triangle mr-1"></i><strong>Alarm</strong></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link " data-toggle="tab" href="#history"><i
                                class="fas fa-history mr-1"></i><strong>History</strong></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" data-toggle="tab" href="#dashboard"><i
                                class="far fa-chart-bar mr-1"></i><strong>Dashboard</strong></a>
                    </li>
                </ul>

                <!-- Tab panes -->
                <div class="tab-content">
                    <div id="mainPage1" class="container-fluid tab-pane active bg-white full-height no-gutter-all">
                        <!-- <div class="alert alert-warning alert-dismissible ml-auto fade show" style = "width:20%">
                            <button type="button" class="close" data-dismiss="alert">&times;</button>
                            <i
                                class="fas fa-exclamation-triangle mr-1"></i><strong>Warning!</strong> New alarm
                        </div> -->
                    </div>
                    <div id="dashboard" class="container-fluid tab-pane fade bg-white full-height"><br>
                    </div>
                    <div id="alarm" class="container-fluid tab-pane fade bg-white full-height">
                        <div class="row pt-3">
                            <button class="btn btn-primary ml-auto mr-5" id="btnAck">Acknowledge</button>
                            <!-- <button class="btn btn-primary mx-3" id="btnAckAll">Acknowledge All</button> -->
                        </div>

                        <div class="row mt-3" style="max-height: 90%; overflow-y:auto">
                            <table class="table table-hover" id="alarmTable">
                                <thead class="bg-secondary text-white">
                                    <tr>
                                        <th>#</th>
                                        <th>Date</th>
                                        <th>Time</th>
                                        <th>Source</th>
                                        <th>Value</th>
                                        <th>Message</th>
                                        <th>Type</th>
                                        <th>State</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td><input type="checkbox"></td>
                                        <td>10/03/2019</td>
