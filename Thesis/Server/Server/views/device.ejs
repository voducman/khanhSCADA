    <!--Create new device-->
    <div class="modal fade" tabindex="-1" role="dialog" id="newDeviceModal" data-backdrop="static">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Create a new device</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="row py-1">
                        <div class="col-sm-4">Device name</div>
                        <div class="col-sm">
                            <input type="text" class="form-control inputName" placeholder="required" required>
                        </div>
                    </div>
                    <div class="row py-1">
                        <div class="col-sm-4">Unique ID</div>
                        <div class="col-sm">
                            <input type="text" class="form-control inputID" placeholder="required" required>
                        </div>
                    </div>
                    <div class="row py-1">
                        <div class="col-sm-4">Longitude</div>
                        <div class="col-sm">
                            <input type="number" class="form-control inputLongitude">
                        </div>
                    </div>
                    <div class="row py-1">
                        <div class="col-sm-4">Latitude</div>
                        <div class="col-sm">
                            <input type="number" class="form-control inputLatitude">
                        </div>
                    </div>
                    <div class="row py-1">
                        <div class="col-sm-4">Data interval</div>
                        <div class="col-sm">
                            <input type="number" class="form-control inputInterval" min="0" placeholder="miliseconds">
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-secondary btnCancel" data-dismiss="modal">Cancel</button>
                    <button type="button" class="btn btn-primary btnNext" data-target="#newPLCModal">Next</button>
                </div>
            </div>
        </div>
    </div>

    <!--Add new PLC-->
    <div class="modal fade" tabindex="-1" role="dialog" id="newPLCModal" data-backdrop="static">
        <div class="modal-dialog modal-lg modal-dialog-scrollable" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Add new PLC</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <fieldset class="form-group border px-4 pb-2 pt-1">
                        <legend class="col-form-label w-auto px-2">PLC information</legend>
                        <div>
                            <div class="row py-1">
                                <div class="col-sm-3">Name</div>
                                <div class="col-sm">
                                    <input type="text" class="form-control inputName" placeholder="required">
                                </div>
                            </div>

                            <div class="row py-1">
                                <div class="col-sm-3">Protocol</div>
                                <div class="col-sm">
                                    <select name="protocol" class="custom-select">
                                        <option selected value="S7-connection">S7 Connection</option>
                                        <!-- <option value="ModbusTCP">Modbus TCP/IP</option> -->
                                        <option value="OPCUA">OPC UA</option>
                                    </select>
                                </div>
                            </div>

                            <div class="row py-1">
                                <div class="col-sm-3">IP address</div>
                                <div class="col-sm">
                                    <input type="text" class="form-control inputIPAddress" placeholder="required">
                                </div>
                            </div>
                            <div class="row py-1">
                                <button class="btn btn-primary btn-block ml-auto mr-3" style="max-width:120px"
                                    id="btnAddNewPLC">Add</button>
                            </div>
                        </div>
                    </fieldset>

                    <fieldset class="form-group border px-4 pb-2 pt-1">
                        <legend class="col-form-label w-auto px-2">PLC list</legend>
                        <div>
                            <div class="row">
                                <button class="btn btn-danger ml-auto mr-3 btn-block mt-0 mb-3 delete-row"
                                    style="max-width:120px">Delete</button>
                            </div>

                            <table class="table table-hover" id="plcTable">
                                <thead>
                                    <tr>
                                        <th>Select</th>
                                        <th>Name</th>
                                        <th>Protocol</th>
                                        <th>IP address</th>
                                    </tr>
                                </thead>
                                <tbody id="plcTableBody" style="max-height:300px">

                                </tbody>
                            </table>
                        </div>
                    </fieldset>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-primary btnNext">Next</button>
                </div>
            </div>
        </div>
    </div>

    <!--Add new variable-->
    <div class="modal fade" tabindex="-1" role="dialog" id="addNewVariable" data-backdrop="static">
        <div class="modal-dialog modal-xl" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Add new variable</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
